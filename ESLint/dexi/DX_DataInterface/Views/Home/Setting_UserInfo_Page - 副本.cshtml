<!-- 弹出message占位 -->
<div id="sign_out_message_id"></div>

   

    
   <!-- 界面js代码 -->
<script type="text/javascript">
    //初始化加载执行-更新用户信息
    window.onload = function () {//要执行的js代码段  
        var obj = parent.GetUserInfoCache();   //调用父窗口函数，获取缓存的用户信息
        if (obj == null) {
            alert('无效的用户信息');               //调试显示信息
        }
        else
        {
            //alert(JSON.stringify(obj, 4));     //调试显示信息
            var info = obj[0];
            //显示用户信息
            document.getElementById('user_name_id').innerHTML = info.NICK_NAME;//用户名称
            if (info.ROLE == 64) //普通用户
            {
                document.getElementById('user_type_id').innerHTML = '普通用户';
            }
            else if (info.ROLE == 256) //管理员
            {
                document.getElementById('user_type_id').innerHTML = '管理员';
            }
            else
            {
                document.getElementById('user_type_id').innerHTML = '未知类型';
            }
            document.getElementById('login_name_id').innerHTML = '用户名：'+info.USER;//登录用户名
            document.getElementById('email_id').innerHTML = '  邮 箱：' + info.EMAIL;//邮箱
            document.getElementById('phone_id').innerHTML = '  电 话：' + info.TEL;//电话
            document.getElementById('addr_id').innerHTML = '  公 司：' + info.COMPANY;//地址

        }
    }

</script>

</body>


</html>

