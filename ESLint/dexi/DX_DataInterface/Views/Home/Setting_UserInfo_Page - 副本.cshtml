<body>

   
    <div class="user_content">
        <div class="row">
            <div class="col-md-6 col-lg-4">
                <div>
                    <div class="profile-photo">
                        <img alt="用户图标" src="../../helsinki-blue/images/user-avatar.jpg"/>
                    </div>
                    <div class="user-header-info">
                        <h2 class="user-name" id="user_name_id">用户名称</h2>
                        <h5 class="user-position" id="user_type_id">用户类型</h5>                           
                    </div>
                </div>
                <div class="panel bg-scale-0 b-primary bt-sm mt-xl">
                    <div class="panel-content">
                        <h4 class=""><b><i class="color-primary mr-sm fa fa-info"></i>用户信息</b></h4>
                        <ul class="user-contact-info ph-sm">
                            <li><b><i class="color-primary mr-sm fa fa-user"></i></b><a id="login_name_id">登录用户名</a> </li>
                            <li><b><i class="color-primary mr-sm fa fa-envelope"></i></b><a id="email_id">邮箱</a></li>
                            <li><b><i class="color-primary mr-sm fa fa-phone"></i></b><a id="phone_id">电话</a></li>
                            <li><b><i class="color-primary mr-sm fa fa-globe"></i></b><a id="addr_id">地址</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

                    



    

<script src="../../helsinki-blue/javascripts/jquery.min.js"></script>
