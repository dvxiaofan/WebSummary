    function add_user_onclick()
    {
        return false;  
    }

    //管理员获取所有普通用户的名称与登录名称列表

    //获取用户信息
    function GetAllUserNameList() {
        var obj = null;

        //请求服务器进行登录
        var jsonData = {
            GetFun: 'GetAllUserNameList',
        };

        $.ajax({
            url: 'Index',
            type: 'POST',
            dataType: 'json',
            data: jsonData,
        })
        .done(function (response) {
            if (response.rel == 1) { //获取成功
                obj = JSON.parse(response.obj);                 //全局缓存用户信息
                alert(JSON.stringify(response.obj, 4));         //调试显示信息
                parent.Set_g_UserNameList(obj);                 //记录全局的用户列表信息，下次访问就可以不用重新获取了
            }
        })
        .fail(function () {
            
        })
    }

</script>

