<script type="text/javascript">

    
    //初始化加载执行
    window.onload = function () {//要执行的js代码段  
        var obj;

        obj = parent.Get_g_UserNameList();  //访问父窗口的全局变量
        if(obj == null) //没有获取过用户信息
        {
            GetAllUserNameList();     //获取用户信息-获取到了就会写入到全局缓冲区中，下次就不用获取了
        }
    }

    //搜索按钮处理
