    <style type="text/css">
        /*INFO 按钮蓝色*/
        .btn-info {
            background-color: #3E79BB;
            border-color: #3E79BB;
        }

        /*INFO 按钮禁用淡蓝色*/
        .btn-info.disabled,
        .btn-info[disabled] {
            background-color: #88bffc;
            border-color: #88bffc;
        }

        /*表格字体颜色，黑色*/
        .layui-table {
            color: #232323;
        }

        .laytable-cell-1-8 {
            width: 365px !important;
        }
