
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>视频播放</title>
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <link rel="shortcut icon" href="/favicon.ico">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link href="../../helsinki-blue/user/Light7/css/light7.css" rel="stylesheet" />
    <link href="../../helsinki-blue/user/layui/css/layui.css" rel="stylesheet" />


</head>
<body>
    

    
    <style type="text/css">
        .iframe-video-player {
            height: 100%;
            width: 100%;
            display: block;
            margin-top: 50px;
            border: none;
        }

    </style>



    <div class="page-group">

        <div class="page">

            <header class="bar bar-nav">
                <button style="font-size:14px;" onclick="back_onclick()" class="button button-link button-nav pull-left">
                <span class="icon icon-left"></span>
                    返回
                  </button>
              

                <!-- <a  class="icon icon-left button button-link button-nav pull-left" style="height:32px; font-size:18px;z-index:100;background-color:#f7f7f7;">返回</a>
                -->
             <h1 class="title" style="margin-left:0px;" id="title_st_id">查看视频</h1>
            </header>


            <div>
                <iframe id="iframe_video_player" class="iframe-video-player" src="Video_LoadingTips_Page" name="device_data" onscroll="no"></iframe>
            </div>

        </div>
    </div>




     <!-- 注意SUI与jquery冲突 -->
    <script src="../../helsinki-blue/javascripts/jquery.min.js"></script>
    <script src="../../helsinki-blue/user/layer/layer.js"></script>
    <script src="../../helsinki-blue/user/layui/layui.js"></script>
    <script src="../../helsinki-blue/user/Light7/light7.js?verson=2"></script>


    <script>

        
        // 返回
        function back_onclick() {
            try {
                var obj = document.getElementById('iframe_video_player');
                obj.style.display = 'none';
                obj.src = 'Video_LoadingTips_Page';

                parent.iframe_show_video_list(); //显示视频列表列表

            } catch (e) {
                layer.msg("处理数据发生了错误：" + e.message, { icon: 5, scrollbar: false }); //5：失败；6：成功
            }

            
        }

        //播放视频,对iframe URL进行URL
        function SetIframeURL(url)
        {
            try {
                
                var obj = document.getElementById('iframe_video_player');     //获取容器
                obj.style.display = 'inline'; //显示视频列表
                obj.src = url;
            } catch (e) {
                layer.msg("处理数据发生了错误：" + e.message, { icon: 5, scrollbar: false }); //5：失败；6：成功
            }
        }




       
    </script>



</body>
</html>
